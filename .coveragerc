[report]
exclude_lines =
    pragma: no cover
    def __str__

# Exclude django-generated files and settings file
omit =
    howsmytrack/asgi.py
    howsmytrack/jobs.py
    howsmytrack/settings.py
    howsmytrack/wsgi.py
    howsmytrack/core/admin.py
    # Script file to be run locally, never on prod.
    howsmytrack/core/management/commands/generate_stats_csvs.py
